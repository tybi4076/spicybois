library(tidyverse)
source("http://www.openintro.org/stat/data/cdc.R")
cdc <- as_tibble(cdc)
ggplot(data = cdc, (mapping =  aes(x = exerany))) +
stat_count()+
facet_grid(.~genhlth)+
labs(title = "Whether People Excercised in the Past Month, and How They Describe Their Health")+
